<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andriod_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Andriod_Project">
        <activity android:name=".ChallengeModeActivity"></activity>
        <activity android:name=".JoinActivity"></activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainHomeActivity" />
        <activity android:name=".StroyModeActivity" />
        <activity android:name=".ChallengeAreaActivity" />
        <activity android:name=".StudyAreaActivity" />
        <activity android:name=".ModeSelectionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>